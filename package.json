{
  "name": "cone-counter",
  "version": "1.0.0",
  "description": "A webapp to track and analyze bong/cone usage over time",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec ts-node src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "start": "node dist/server.js"
  },
  "keywords": [
    "bong",
    "cone",
    "tracker",
    "analytics",
    "webapp"
  ],
  "author": "Alex Schladetsch",
  "license": "MIT",
  "dependencies": {
    "all": "^0.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
